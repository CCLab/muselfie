<Page class="page" navigatingTo="onNavigatingTo" xmlns="http://schemas.nativescript.org/tns.xsd">
    <Page.actionBar>
        <ActionBar class="action-bar">
            <StackLayout class="steps" orientation="horizontal" horizontalAlignment="left">
                <Label class="logo"/>
                <Label class="app-name" text="muselfie"/>
                <Label class="info"/>
            </StackLayout>
        </ActionBar>
     </Page.actionBar>
    <GridLayout rows=" *, auto" class="home-page-layout">
        <Image row="0" class="start-page-image" src="res://start_v2_fs8_small2"/>
        <FlexboxLayout row="1" class="navigation-buttons-container">
            <StackLayout class="btn-new-wrapper" tap="startTap" orientation="horizontal">
                <Label class="btn-new-text" text="rozpocznij"/>
                <Label class="btn-new"/>
            </StackLayout>
        </FlexboxLayout>
    </GridLayout>
</Page>
